<h1>Sign Up</h1>

<div>
    <form action="<%= users_url %>" method="post">
        <%= auth_token %>
        <label>Username:
            <input name="user[username]" value = "<%= @user.username  %>" >
        </label>
        <label>Password:
            <input name="user[password]" value = "<%= @user.password  %>">
        </label>
        <button type="submit">Sign Up
    </form>

</div>